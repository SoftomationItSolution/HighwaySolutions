<Window x:Class="LaneApplication.LogIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LaneApplication"
        mc:Ignorable="d"
        Title="LogIn" Height="214.195" Width="300" WindowStyle="SingleBorderWindow">
    <Canvas>
        <Label x:Name="labelUserName" Content="User Name:" Canvas.Left="39" Canvas.Top="56"/>
        <Label x:Name="labelPassword" Content="Password:" Canvas.Left="39" Canvas.Top="87"/>
        <TextBox x:Name="textBoxUserName" Height="23" Canvas.Left="121" TextWrapping="Wrap" Text="TextBox" Canvas.Top="59" Width="151"/>
        <PasswordBox x:Name="passwordBoxPassword" Canvas.Left="121" Canvas.Top="95" Width="151"/>
        <Button x:Name="buttonLogIn" Content="Login" Canvas.Left="72" Canvas.Top="134" Width="75"/>
        <Button x:Name="buttonCancel" Content="Cancel" Canvas.Left="170" Canvas.Top="134" Width="75"/>


    </Canvas>
</Window>
